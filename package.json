    "test": "mocha --compilers coffee:coffee-script spec/*.coffee"
  },
  "dependencies": {
    "async": "0.2.8",
    "underscore": "1.4.4"
  },
  "devDependencies": {
    "chai": "1.6.0",
    "jscov": "0.5.3",
    "mocha": "1.10.0",
    "mocha-term-cov-reporter": "0.1.0",
    "coffee-script": "1.6.2"
  }
}
