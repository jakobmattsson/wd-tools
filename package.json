{
  "name": "wd-tools",
  "version": "0.1.0",
  "description": "Utility functions for Selenium WebDriver",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "main": "./lib/wd-tools.js",
  "engines": {
    "node": ">= 0.8"
  },
  "scripts": {
    "pretest": "coffee -co lib src",
    "prepublish": "coffee -co lib src",
